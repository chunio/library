{
  "name": "baichuan/library",
  "type": "library",
  "description": "a common library for [php]hyperf",
  "keywords": ["php", "hyperf", "common", "library"],
  "license": "MIT",
  "archive": {
    "exclude": [
      "/bin",
      "*.md",
      "/.*"
    ]
  },
  "require": {
    "php": ">=7.4",
    "hyperf/cache": "~2.2.0",
    "hyperf/command": "~2.2.0",
    "hyperf/config": "~2.2.0",
    "hyperf/db-connection": "~2.2.0",
    "hyperf/framework": "~2.2.0",
    "hyperf/guzzle": "~2.2.0",
    "hyperf/http-server": "~2.2.0",
    "hyperf/logger": "~2.2.0",
    "hyperf/memory": "~2.2.0",
    "hyperf/process": "~2.2.0",
    "hyperf/database": "~2.2.0",
    "hyperf/redis": "~2.2.0",
    "hyperf/constants": "~2.2.0",
    "hyperf/async-queue": "~2.2.0",
    "hyperf/amqp": "~2.2.0",
    "hyperf/model-cache": "~2.2.0",
    "hyperf/elasticsearch": "~2.2.0",
    "hyperf/tracer": "~2.2.0",
    "hyperf/validation": "^2.2",
    "hyperf/resource": "~2.2.0",
    "hyperf/gotask": "^2.2",
    "geoip2/geoip2": "^2.13",
    "hyperf/kafka": "^2.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "hyperf/devtool": "~2.2.0",
    "hyperf/ide-helper": "~2.2.0",
    "hyperf/testing": "~2.2.0",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^0.12",
    "swoole/ide-helper": "^4.5",
    "hyperf/watcher": "v2.2.6.1"
  },
  "autoload": {
    "files": [
      "src/Utility/Common.php"
    ],
    "psr-4": {
      "Baichuan\\Library\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "hyperf": {
      "config": "Baichuan\\Library\\ConfigProvider"
    }
  }
}
